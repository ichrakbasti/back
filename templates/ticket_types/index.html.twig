{% extends 'base.html.twig' %}

{% block title %}TicketTypes index{% endblock %}

{% block body %}
    <h1>TicketTypes index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>TypeName</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for ticket_type in ticket_types %}
            <tr>
                <td>{{ ticket_type.id }}</td>
                <td>{{ ticket_type.typeName }}</td>
                <td>
                    <a href="{{ path('app_ticket_types_show', {'id': ticket_type.id}) }}">show</a>
                    <a href="{{ path('app_ticket_types_edit', {'id': ticket_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_ticket_types_new') }}">Create new</a>
{% endblock %}
